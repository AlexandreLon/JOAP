    "compilerOptions": {
      "module": "CommonJS",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "target": "ES2021",
      "sourceMap": true,
      "incremental": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "outDir": "./dist",
       "baseUrl": "./",
    },
  }
  